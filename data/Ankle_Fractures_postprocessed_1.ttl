    ns1:diagnosisMethod ns2:MRI,
        ns2:XRay ;
    ns1:evidentOn ns2:AnkleXRay ;
    ns1:partOf ns2:DiagnosisOfAnkleFractures ;
    ns1:relatedTo ns2:AnkleRing,
        ns2:Disruption ;
    ns1:stability ns2:Stable,
        ns2:Unstable ;
    ns1:treatment ns2:Casting,
        ns2:ORIF,
        ns2:WalkingBoot .

ns2:AnklePain a ns1:Symptom ;
    ns1:code [ a ns1:CodeableConcept ;
            ns1:coding [ a ns1:Coding ;
                    ns1:code "247373008" ;
                    ns1:display "Ankle pain" ;
                    ns1:system "http://snomed.info/sct" ] ] ;
    ns1:partOf ns2:OttawaAnkleRules .

ns2:AnkleStabilityEvaluation a ns1:Procedure ;
    ns1:definition "Assessment of the stability of the ankle, which determines treatment." ;
    ns1:description "ankle stability evaluation" ;
    ns1:relatedTo ns2:PhysicalExamination,
        ns2:XRay .

ns2:AnteriorTalofibularLigament a ns1:BodyStructure ;
    ns1:partOf ns2:LateralLigamentComplex .

ns2:BoneTenderness a ns1:Observation ;
    ns1:description "Bone tenderness within 6 cm of the posterior edge or tip of either malleolus" ;
    ns1:partOf ns2:OttawaAnkleRules .

