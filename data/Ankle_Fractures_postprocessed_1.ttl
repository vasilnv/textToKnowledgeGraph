ns2:AgeOver55 a ns1:Observation ;
    ns1:partOf ns2:OttawaAnkleRules ;
    ns1:valueString "Age >55" .

ns2:AnkleFracture a ns1:Condition ;
    ns1:bodySite ns2:LateralMalleolus,
        ns2:MedialMalleolus,
        ns2:PosteriorMalleolus ;
    ns1:causedBy ns2:InversionInjury,
        ns2:Jumping,
        ns2:Running ;
    ns1:code [ a ns1:CodeableConcept ;
            ns1:coding [ a ns1:Coding ;
                    ns1:code "16114001" ;
